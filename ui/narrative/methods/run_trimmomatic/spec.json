{
	"ver": "0.0.12",
	
	"authors": [
		"psdehal",
		"dylan"
	],
	"contact": "help@kbase.us",
	"visible": true,
	"categories": ["active", "reads" ],
	"widgets": {
		"input": null,
		"output": "kbaseReportView"
	},
	"parameters": [
		{
			"id": "read_type",
			"optional": false,
			"advanced": false,
			"allow_multiple": false,
			"default_values": [ "PE" ],
			"field_type": "dropdown",
			"dropdown_options": {
				"options": [
					{
						"value": "PE",
						"display": "Paired End",
						"id": "PE",
						"ui-name": "PE"
					},
					{
						"value": "SE",
						"display": "Single End",
						"id": "SE",
						"ui-name": "SE"
					}
				]
			}
		},
		{
			"id": "input_reads_ref",
			"optional": false,
			"advanced": false,
			"allow_multiple": false,
			"default_values": [ "" ],
			"field_type": "text",
			"text_options": {
				"valid_ws_types": ["KBaseSets.ReadsSet", "KBaseFile.PairedEndLibrary", "KBaseAssembly.PairedEndLibrary", "KBaseAssembly.SingleEndLibrary", "KBaseFile.SingleEndLibrary"]
			}
		},
		{
			"id": "output_reads_name",
			"is_output_name": true,
			"optional": false,
			"advanced": false,
			"allow_multiple": false,
			"default_values": [ "" ],
			"field_type": "text"
		},
		{
			"id": "quality_encoding",
			"optional": false,
			"advanced": false,
			"allow_multiple": false,
			"default_values": [ "phred33" ],
			"field_type": "dropdown",
				"dropdown_options": {
					"options": [
						{
							"value": "phred33",
							"display": "phred33 (Sanger, 454, SRA, Illumina >= 1.8)",
							"id": "phred33",
							"ui-name": "phred33"
						},
						{
							"value": "phred64",
							"display": "phred64 (Illumina <= 1.7)",
							"id": "phred64",
							"ui-name": "phred64"
						}
					]
				}
		},

		{
			"id": "adapterFa",
			"optional": true,
			"advanced": true,
			"allow_multiple": false,
			"default_values": [""],
			"field_type": "dropdown",
				"dropdown_options": {
					"options": [
						{
							"value": "TruSeq3-PE.fa",
							"display": "TruSeq3-PE",
							"id": "TruSeq3-PE",
							"ui-name": "TruSeq3-PE"
						},
						{
							"value": "TruSeq2-PE.fa",
							"display": "TruSeq2-PE",
							"id": "TruSeq2-PE",
							"ui-name": "TruSeq2-PE"
						},
						{
							"value": "TruSeq2-PE-2.fa",
							"display": "TruSeq2-PE-2",
							"id": "TruSeq2-PE-2",
							"ui-name": "TruSeq2-PE-2"
						},
						{
							"value": "TruSeq3-SE.fa",
							"display": "TruSeq3-SE",
							"id": "TruSeq3-SE",
							"ui-name": "TruSeq3-SE"
						},
						{
							"value": "TruSeq2-SE.fa",
							"display": "TruSeq2-SE",
							"id": "TruSeq2-SE",
							"ui-name": "TruSeq2-SE"
						},
						{
							"value": "NexteraPE-PE.fa",
							"display": "NexteraPE-PE",
							"id": "NexteraPE-PE",
							"ui-name": "NexteraPE-PE"
						}
					]
				}
		},
		{
			"id": "seed_mismatches",
			"optional": true,
			"advanced": true,
			"allow_multiple": false,
			"default_values": [ "2" ],
			"field_type": "text",
			"text_options": {
				"validate_as": "int"
			}
		},
		{
			"id": "palindrome_clip_threshold",
			"optional": true,
			"advanced": true,
			"allow_multiple": false,
			"default_values": ["3"],
			"field_type": "text",
			"text_options": {
				"validate_as": "int"
			}
		},
		{
			"id": "simple_clip_threshold",
			"optional": true,
			"advanced": true,
			"allow_multiple": false,
			"default_values": [ "10" ],
			"field_type": "text",
			"text_options": {
				"validate_as": "int"
			}
		},

		{
			"id": "sliding_window_size",
			"optional": true,
			"advanced": true,
			"allow_multiple": false,
			"default_values": [ "4" ],
			"field_type": "text",
			"text_options": {
				"validate_as": "int"
			}
		},
		{
			"id": "sliding_window_min_quality",
			"optional": true,
			"advanced": true,
			"allow_multiple": false,
			"default_values": [ "15" ],
			"field_type": "text",
			"text_options": {
				"validate_as": "int"
			}
		},

		{
			"id": "crop_length",
			"optional": true,
			"advanced": true,
			"allow_multiple": false,
			"default_values": [ "0" ],
			"field_type": "text",
			"text_options": {
				"validate_as": "int"
			}
		},
		{
			"id": "head_crop_length",
			"optional": true,
			"advanced": true,
			"allow_multiple": false,
			"default_values": [ "0" ],
			"field_type": "text",
			"text_options": {
				"validate_as": "int"
			}
		},
		{
			"id": "leading_min_quality",
			"optional": true,
			"advanced": true,
			"allow_multiple": false,
			"default_values": [ "3" ],
			"field_type": "text",
			"text_options": {
				"validate_as": "int"
			}
		},
		{
			"id": "trailing_min_quality",
			"optional": true,
			"advanced": true,
			"allow_multiple": false,
			"default_values": [ "3" ],
			"field_type": "text",
			"text_options": {
				"validate_as": "int"
			}
		},
		{
			"id": "min_length",
			"optional": true,
			"advanced": true,
			"allow_multiple": false,
			"default_values": [ "36" ],
			"field_type": "text",
			"text_options": {
				"validate_as": "int"
			}
		}
	],
	"parameter-groups": [
	    {
	      "id": "adapter_clip",
	      "parameters": ["adapterFa", "seed_mismatches", "palindrome_clip_threshold", "simple_clip_threshold"],
	      "optional": true,
	      "allow_multiple": false,
	      "with_border": true
	    },
	    {
	      "id": "sliding_window",
	      "parameters": ["sliding_window_size", "sliding_window_min_quality"],
	      "optional": true,
	      "allow_multiple": false,
	      "with_border": true
	    },
	    {
	      "id": "others",
	      "parameters": ["leading_min_quality", "trailing_min_quality", "crop_length", "head_crop_length", "min_length"],
	      "optional": true,
	      "allow_multiple": false,
	      "with_border": true
	    }
	],

	"behavior": {
		"service-mapping": {
			"url": "",
			"name": "kb_trimmomatic",
			"method": "runTrimmomatic",
			"input_mapping": [
				{
					"narrative_system_variable": "workspace",
					"target_property": "input_ws"
				},
				{
					"input_parameter": "read_type",
					"target_property": "read_type"
				},
				{
					"input_parameter": "input_reads_ref",
					"target_property": "input_reads_ref",
					"target_type_transform": "resolved-ref"

				},
				{
					"input_parameter": "quality_encoding",
					"target_property": "quality_encoding"
				},
				{
					"input_parameter": "adapter_clip",
					"target_property": "adapter_clip"
				},
				{
					"input_parameter": "sliding_window",
					"target_property": "sliding_window"
				},
				{
					"input_parameter": "others",
					"target_property": "others"
				}

			],
			"output_mapping": [
				{
					"service_method_output_path": [0, "report_name"],
					"target_property": "report_name"
				},
				{
					"service_method_output_path": [0, "report_ref"],
					"target_property": "report_ref"
				},
				{
					"narrative_system_variable": "workspace",
					"target_property": "workspace_name"
				}
			]
		}
	},
	"job_id_output_field": "docker"
}
